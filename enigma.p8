--explosions

local explo={}

function create_explo(x, y)
	sfx(6)
	new_explo={
		x=x,
		y=y,
		timer=0
		}
	add(explo, new_explo)
end

function update_explo()

	for e in all(explo) do
		e.timer += 1
		if e.timer == 13 then
			del(explo, e)
			end
		end
end


function draw_explo()
	for e in all(explo) do
		circ(e.x, e.y, 
						e.timer/3, 8+e.timer%3)
	end
end



