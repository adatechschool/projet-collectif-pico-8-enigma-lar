--bullets

local blt={}

function shoot()
	local new_blt={
	 x=plr.x,
	 y=plr.y,
	 direction=plr.flip and -1 or 1,
  sprite=48,
  speed=1,
  timer=60
 }
 add(blt, new_blt)
end


function update_blt()
	for b in all(blt) do
  b.x = b.x + b.direction * b.speed
  b.timer-=1
  if b.timer <= 0 then
  	del(blt, b)
  end
 end
end


function draw_blt()
	for b in all(blt) do
			spr(b.sprite,b.x*8,b.y*8)
	end
end

